  "scripts": {
    "start": "NODE_OPTIONS=--max-http-header-size=81920 next start",
    "start:collaborative": "node ./build/collaborative/sharedb/server.js",
    "build": "next build",
    "build:collaborative": "tsc -p tsconfig.collaborative.json",
    "dev": "NODE_OPTIONS='--inspect --max-http-header-size=81920' next dev -H 0.0.0.0",
    "dev:collaborative": "tsc-watch -p \"./tsconfig.collaborative.json\" --onCompilationComplete  \"yarn run start:collaborative\"",
    "test": "NODE_ENV=test TZ=UTC jest",
    "test:watch": "yarn run test --watch",
    "test:cov": "yarn run test --coverage",
    "test:verbose": "yarn run test:cov --verbose",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "yarn run lint --fix",
    "lint:error": "yarn run lint --max-warnings 0",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "yarn run typecheck --watch",
    "format": "prettier --write './**/*.@(js|jsx|ts|tsx)'",
    "format:check": "prettier --check './**/*.@(js|jsx|ts|tsx)'"
  },
